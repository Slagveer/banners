package  {		import flash.display.MovieClip;	import com.greensock.TweenLite;	import com.greensock.easing.*;	import flash.utils.*;	import flash.net.*;	import flash.events.MouseEvent;	import flash.external.ExternalInterface;		public class main336x280 extends MovieClip {				var self = this;				public function main336x280() {			// constructor code						var mc:MovieClip = new MovieClip();			mc.graphics.beginFill(0);			mc.graphics.drawRect(0,0,336,280);			mc.graphics.endFill();			mc.alpha = 0;						mc.buttonMode = true;						mc.addEventListener(MouseEvent.CLICK, function(evt){				var url:String = "http://online-djschool.nl";				var request:URLRequest = new URLRequest(url);				try {					navigateToURL(request, '_blank');					//ExternalInterface.call("window.open", request, '_blank');				} catch (e:Error) {					trace("Error occurred!");				}			});						addChild(mc);						record.x = 168;			record.alpha = 0;			record.scaleX = 1.3;			record.scaleY = 1.3;						hand.alpha = 0;			hand.scaleX = 0.15;			hand.scaleY = 0.15;			hand.x = 220; 			hand.y = 140;						title.alpha = 0;			title.x = 168;			title.scaleX = 0.5;			title.scaleY = 0.5;						tafel.x = 168;			tafel.y = 400;			tafel.scaleX = .8;			tafel.scaleY = .8;			tafel.titel.scaleX = 0;			tafel.titel.scaleY = 0;						dj.alpha = 1;			dj.x = 168;			dj.y = 400;			dj.scaleX = 3;			dj.scaleY = 3;						djtitle.alpha = 1;			djtitle.x = 168;			djtitle.y = -40;			djtitle.scaleX = 1;			djtitle.scaleY = 1;						laptop.x = 520;			laptop.y = 140;			laptop.scaleX = .25;			laptop.scaleY = .25;						laptoptitle.x = 98;			laptoptitle.y = -40;						geldtitle.x = 50;			geldtitle.y = -40;			geldtitle.scaleX = 1;			geldtitle.scaleY = 1;						mensenlinks.x = 10;			mensenlinks.y = 600;						mensenrechts.x = 330;			mensenrechts.y = 600;						startRecord();			//startLaptop();			//startGeldTitle();			//reStartTafel();			//startTafel();			//startMensen();		}				function startRecord(){			TweenLite.to(record, 1, {alpha:1,y:150, onComplete:function(){						startHand();					}				});		}				function stopRecord(){			TweenLite.to(record, 2, {y:425, ease: ElasticInOut.ease, onComplete:function(){						stopTitle();					}				});		}				function startHand(){			TweenLite.to(hand, 2, {alpha:1, y:170, ease: ElasticOut.ease, onComplete:function(){						//hand.stop();						setInterval(handAction,500);						handAction();						startTitle();					}				});		}				function stopHand(){			TweenLite.to(hand, 2, {y:425, delay:3, ease: ElasticInOut.ease, onComplete:function(){						stopRecord();					}				});		}				function startTitle(){			TweenLite.to(title, 1, {alpha:1, y: 40, onComplete:function(){						stopHand();					}				});		}				function stopTitle(){			TweenLite.to(title, 2, {alpha:1, y: -40, ease: ElasticInOut.ease, onComplete:function(){						startTafel();					}				});		}				function startTafel(){			TweenLite.to(tafel, 1, {alpha:1, y: 240, onComplete:function(){						TweenLite.to(tafel.titel, .5, {scaleX :1,scaleY :1});						startDj();					}				});		}				function stopTafel(){			TweenLite.to(tafel, 2, {alpha:1, y: 600, ease: ElasticInOut.ease, onComplete:function(){						stopDjTitle();					}				});		}				function reStartTafel(){			TweenLite.to(tafel, 1, {alpha:1, y: 240, onComplete:function(){						reStartDj();					}				});		}				function startDj(){			TweenLite.to(dj, .5, {alpha:1, y: 90, onComplete:function(){						startDjTitle();					}				});		}				function stopDj(){			TweenLite.to(dj, 1, {alpha:1, y: 400, delay: 5, onComplete:function(){						stopTafel();					}				});		}				function reStartDj(){			TweenLite.to(dj, .5, {alpha:1, y: 90, onComplete:function(){						startGeldTitle();					}				});		}				function startDjTitle(){			TweenLite.to(djtitle, .5, {alpha:1, delay:1, y: 30, onComplete:function(){						stopDj();					}				});		}				function stopDjTitle(){			TweenLite.to(djtitle, 2, {alpha:2, y: -40, ease: ElasticInOut.ease , onComplete:function(){						startLaptop();					}				});		}				function startLaptop(){			TweenLite.to(laptop, .5, {alpha:1, x: 168, onComplete:function(){						startLaptopTitle();					}				});		}				function stopLaptop(){			TweenLite.to(laptop.image, 2, {x: -1320, delay: 3,ease: ElasticInOut.ease , onComplete:function(){						stopLaptopTitle();					}				});		}				function startLaptopTitle(){			TweenLite.to(laptoptitle, .5, {alpha:1, delay:1, y: 30, onComplete:function(){						stopLaptop();					}				});		}				function stopLaptopTitle(){			TweenLite.to(laptoptitle, 2, {alpha:2, y: -40, ease: ElasticInOut.ease , onComplete:function(){						TweenLite.to(laptop.bg, 1, {alpha: 0});						reStartTafel();					}				});		}				function startGeldTitle(){			TweenLite.to(geldtitle, .5, {alpha:1, delay:1, y: 30, onComplete:function(){						startMensen();					}				});		}				function startMensen(){			TweenLite.to(mensenlinks, .5, {alpha:1, y: 240, onComplete:function(){						//;					}				});							TweenLite.to(mensenrechts, .5, {alpha:1, y: 240, onComplete:function(){					setTimeout(function(){ 							   dj.koptelefoon.stop();							   dj.lichaam.stop();							   mensenlinks.stop();							   mensenrechts.stop();						}, 5000);				}			});		}				function handAction(){			if(Math.random() * 10 > 5){				self.hand.stop();			}else{				self.hand.play();			}		}	}	}