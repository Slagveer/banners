package  {		import flash.display.MovieClip;	import com.greensock.TweenLite;	import com.greensock.easing.*;	import flash.utils.*;	import flash.net.*;	import flash.events.MouseEvent;	import flash.external.ExternalInterface;		public class main200x200 extends MovieClip {				var self = this;				public function main200x200() {			// constructor code						var mc:MovieClip = new MovieClip();			mc.graphics.beginFill(0);			mc.graphics.drawRect(0,0,250,250);			mc.graphics.endFill();			mc.alpha = 0;						mc.buttonMode = true;						mc.addEventListener(MouseEvent.CLICK, function(evt){				var url:String = "http://online-djschool.nl";				var request:URLRequest = new URLRequest(url);				try {					navigateToURL(request, '_blank');					//ExternalInterface.call("window.open", request, '_blank');				} catch (e:Error) {					trace("Error occurred!");				}			});						addChild(mc);						info.scaleX = 0;			info.scaleY = 0;			//info.y = 100;						record.x = 100;			record.alpha = 0;			record.scaleX = 1.2;			record.scaleY = 1.2;						hand.alpha = 0;			hand.scaleX = 0.16;			hand.scaleY = 0.16;			hand.x = 140; 			hand.y = 140;						title.alpha = 0;			title.x = 100;			title.scaleX = 0.3;			title.scaleY = 0.3;						tafel.x = 100;			tafel.y = 400;			tafel.scaleX = .8;			tafel.scaleY = .8;			tafel.titel.scaleX = 0;			tafel.titel.scaleY = 0;						dj.alpha = 1;			dj.x = 100;			dj.y = 400;			dj.scaleX = 3;			dj.scaleY = 3;						djtitle.alpha = 1;			djtitle.x = 90;			djtitle.y = -40;			djtitle.scaleX = 1;			djtitle.scaleY = 1;						laptop.x = 520;			laptop.y = 120;			laptop.scaleX = .20;			laptop.scaleY = .20;						laptoptitle.x = 25;			laptoptitle.y = -40;						geldtitle.x = 8;			geldtitle.y = -40;			geldtitle.scaleX = .8;			geldtitle.scaleY = .8;						mensenlinks.x = 10;			mensenlinks.y = 400;						mensenrechts.x = 150;			mensenrechts.y = 400;						startRecord();			//startLaptop();			//startGeldTitle();			//reStartTafel();			//startTafel();			//startMensen();		}				function startRecord(){			TweenLite.to(record, 1, {alpha:1,y:125, onComplete:function(){						startHand();					}				});		}				function stopRecord(){			TweenLite.to(record, 1, {y:325, delay:3, onComplete:function(){											}				});				stopTitle();		}				function startHand(){			TweenLite.to(hand, 2, {alpha:1, y:140, ease: ElasticOut.ease, onComplete:function(){						//hand.stop();						setInterval(handAction,500);						handAction();											}				});				startTitle();		}				function stopHand(){			TweenLite.to(hand, 1, {y:325, delay:3, onComplete:function(){											}				});				stopRecord();		}				function startTitle(){			TweenLite.to(title, 1, {alpha:1, delay:2, y: 40, onComplete:function(){						stopHand();					}				});		}				function stopTitle(){			TweenLite.to(title, 2, {alpha:1, delay: 3, y: -40, ease: ElasticInOut.ease, onComplete:function(){						startTafel();					}				});		}				function startTafel(){			TweenLite.to(tafel, 1, {alpha:1, y: 220, onComplete:function(){						TweenLite.to(tafel.titel, .5, {scaleX :1,scaleY :1});						startDj();					}				});		}				function stopTafel(){			TweenLite.to(tafel, 2, {alpha:1, delay:3, y: 400, onComplete:function(){											}				});				stopDjTitle();		}				function reStartTafel(){			TweenLite.to(tafel, 1, {alpha:1, y: 220, onComplete:function(){						reStartDj();					}				});		}				function startDj(){			TweenLite.to(dj, .5, {alpha:1, y: 90, onComplete:function(){						startDjTitle();					}				});		}				function stopDj(){			TweenLite.to(dj, 1, {alpha:1, y: 400, delay: 3, onComplete:function(){						stopInfo();					}				});				stopTafel();		}				function reStartDj(){			TweenLite.to(dj, .5, {alpha:1, y: 90, onComplete:function(){						startGeldTitle();					}				});		}				function startDjTitle(){			TweenLite.to(djtitle, .5, {alpha:1, delay:1, y: 30, onComplete:function(){						stopDj();						startInfo();					}				});		}				function stopDjTitle(){			TweenLite.to(djtitle, 2, {alpha:2, delay:4, y: -40, ease: ElasticInOut.ease , onComplete:function(){						startLaptop();					}				});		}				function startLaptop(){			TweenLite.to(laptop, .5, {alpha:1, x: 100, onComplete:function(){						startLaptopTitle();					}				});		}				function stopLaptop(){			TweenLite.to(laptop.image, .4, {x: -1020, delay: 3, onComplete:function(){						stopLaptopTitle();					}				});		}				function startLaptopTitle(){			TweenLite.to(laptoptitle, .5, {alpha:1, delay:1, y: 30, onComplete:function(){						stopLaptop();					}				});		}				function stopLaptopTitle(){			TweenLite.to(laptoptitle, 2, {alpha:2, y: -40, ease: ElasticInOut.ease , onComplete:function(){						TweenLite.to(laptop.bg, 1, {alpha: 0});						reStartTafel();					}				});		}				function startGeldTitle(){			TweenLite.to(geldtitle, .5, {alpha:1, delay:1, y: 30, onComplete:function(){						startMensen();						startInfo();					}				});		}				function startMensen(){			TweenLite.to(mensenlinks, .5, {alpha:1, y: 180, onComplete:function(){						//;					}				});							TweenLite.to(mensenrechts, .5, {alpha:1, y: 180, onComplete:function(){					setTimeout(function(){ 							   dj.koptelefoon.stop();							   dj.lichaam.stop();							   mensenlinks.stop();							   mensenrechts.stop();						}, 5000);				}			});		}				function startInfo(){			TweenLite.to(info, .5, {alpha:1, scaleX:.5, scaleY:.5});		}				function stopInfo(){			TweenLite.to(info, .5, {alpha:0, scaleX:0, scaleY:0});		}				function handAction(){			if(Math.random() * 10 > 5){				self.hand.stop();			}else{				self.hand.play();			}		}	}	}